{
  "name": "IZI",
  "productName": "IZI",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "dependencies": {
    "pretty-bytes": "^2.0.1"
  },
  "devDependencies": {
    "electron": "^1.4.3",
    "electron-prebuilt": "^0.35.2",
    "electron-packager": "^8.1.0"
  },
  "scripts": {
    "start": "electron main.js",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./app/static/img/favicon.ico --prune=true --out=release-builds",
    "package-win": "electron-packager . maydana --overwrite --asar=true --icon=./app/static/img/favicon.ico --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=IZI --version-string.FileDescription=IZI --version-string.ProductName=\"IZI Software\"",
    "package-linux": "electron-packager . maydana --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
  },
  "author": "Matheus Maydana",
  "license": "ISC"
}
